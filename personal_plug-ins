   <rule type="103" typeonclickaction="" subtypename="" jsmethod="search();"
     url="http://m.nek8.com/mso/" searchPostData="wd=%keyword%" currentPageCountHolder="%count%" searchKeywordHolder="%keyword%">
     <script>
      function search() {
        var host = "http://m.nek8.com";
        var datalist = $("div.all_tab.top").find("li");

        if (datalist.length == 0) {
          var response = {
            "code": "200",
            "msg": "suc",
            "content": ""
          };
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var href = host + $(this).find("a").attr("href");
          var title = $(this).find("a").attr("title");
          var imgurl = $(this).find("a").find("img.loading").attr("data-original");
          var content = title;
          var obj = {
            "title": title,
            "detailUrl": href,
            "posterUrl": imgurl,
            "content": content
          };
          tmparray.push(obj);
        });
        var response = {
          "code": "200",
          "msg": "suc2",
          "content": JSON.stringify(tmparray)
        };
        return response;
      }
     </script>
   </rule>
