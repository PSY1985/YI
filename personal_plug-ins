   <rule type="101" typeonclickaction="" subtypename="综艺" jsmethod="list();"
     url="https://m.kuaikan66.com/list/Arts______%count%.html" currentpagecountholder="%count%" searchkeywordholder=""> 
     <script>
      function list() {
        var root = document.getElementById("vod_list");
        var datalist = root.getElementsByTagName("li");

        if (datalist.length == 0) {
          var response = { "code": "200", "msg": "suc", "content": "" };
