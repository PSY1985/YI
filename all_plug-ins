    url="http://www.yongjiuzy.cc/?m=vod-type-id-1-pg-%count%.html" currentpagecountholder="%count%" searchkeywordholder="">
    <script>
      function list() {
        var datalist = $("tbody#data_list").find("tr.DianDian");
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var host = "http://www.yongjiuzy.cc";
          var href = $(this).find("td.l").find("a").attr("href");
          var title = $(this).find("td.l").find("a").text().replace(/\s/g, "").trim();
          var imgurl;
          var content = $(this).find("td").next().text();
          var obj = {"title": title, "detailUrl": host + href, "posterUrl": imgurl, "content": content, "itemType": 1};
          if (content.indexOf("伦理") == -1) {
            if (title.length > 0) {
              tmparray.push(obj);
            }
          }
