            var stDetailTmp = stDetail[j].innerHTML;
            stTmp = stTmp + " " + stDetailTmp;
          }
          actor = actor + "<br>" + stTmp;
        }

        var allDownList = new Array();
        var allDownTitleTmp = $("div.play-title a").toArray();
        var allDownListTmp = $(".play-box").toArray();
