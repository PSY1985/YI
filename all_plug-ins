   <rule type="103" typeonclickaction="" subtypename="" jsmethod="search();"
     url="https://www.wandouys.com/index.php?s=vod-search-name-%keyword%-p-%count%.html" currentpagecountholder="%count%" searchkeywordholder="%keyword%"> 
     <script>
      function search() {
        var datalist = $("ul.list-unstyled.vod-item-img.ff-img-215 li").toArray();
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
