          var obj = {"title": title, "detailUrl": href, "posterUrl": imgurl, "content": content};
          tmparray.push(obj);
        }
        var response = {"code": "200", "msg": "suc", "content": JSON.stringify(tmparray)};
        return response;
      }
     </script> 
   </rule> 

