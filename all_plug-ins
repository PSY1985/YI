    <rule type="102" jsmethod="detail();"> 
     <script>
        function detail(){   
          var host="https://wolongzy.net";    
          var posterUrl;     
          var pageTitle =$("p.whitetitle").first().text().replace("名称：","").trim();     
          var allText = $("div.width1200.white").eq(1).find("div").text().replace("剧情介绍","").trim();
          var shortText = allText.substring(0, allText.length > 200 ? 200 : allText.length).trim();   
          var actor = "";
          var actorTitle=$("div.right p").toArray();
            for(var i=0;i<actorTitle.length;i++){
              var actorText = actorTitle[i].innerText;
              actor = actor+"<br>"+actorText;
          }        

          var allDownList = new Array();                 
          var allDownTitleTmp = $("div.width1200.white").eq(2).find("h4").toArray();
          var allDownListTmp = $("div.width1200.white").eq(2).find("div.playlist.wbox").toArray();            
          for (var i =0;i< allDownListTmp.length;  i++) {        
            var titleSort = $(allDownTitleTmp[i]).text().replace("来源：", "");         
            var  downloadTmp =allDownListTmp[i].getElementsByTagName("li");          
            var oneDownList = new Array(); 
            if(i==0)
            var dl=downloadTmp.length/2;  
            else
            var dl=downloadTmp. length;
            for (var j = 0; j < dl-1; j++) {           
              var tmp = downloadTmp[j].getElementsByTagName("input")[0].getAttribute("value");
              var title = tmp.substring(0, tmp.indexOf("$"));
              var href = tmp.substring(tmp.indexOf("$") + 1, tmp.length);       
              var downloadBean = { "name": title, "href": href};
              oneDownList.push(downloadBean);
        }        
          var pageDetail = {"title": titleSort,  "downlistInfos": oneDownList};
          allDownList.push(pageDetail);
        }      
          var pageInfo = {"posterUrl": posterUrl, "title": pageTitle, "allText": allText,"shortText": shortText,  "summary": actor,  "downlist": allDownList};   
          var response = {"code": "200","msg": "suc","content": pageInfo};
     return response;
      }  
    </script> 
    </rule> 

    <rule type="103" typeonclickaction="" subtypename="" jsmethod="search();"
      url="https://wolongzy.net/search.html?searchword=%keyword%"
        currentpagecountholder="%count%" searchkeywordholder="%keyword%">
    <script>
          function search() {
              var datalist = $("ul.videoContent").find("li");
                if (datalist.length == 0) {
                    var response = {"code": "200", "msg": "suc", "content": ""};
                    return response;
                }
                var tmparray = new Array();
                $(datalist).each(function() {
                    var host = "https://wolongzy.net";
                    var href = $(this).find("a.videoName").attr("href");
                    var title = $(this).find("a.videoName").text().replace(/\s/g, "").trim();
                    var imgurl;
                    var content = $(this).find("span.category").text();
                    var obj = {"title": title, "detailUrl": host + href, "posterUrl": imgurl, "content": content, "itemType":1};
                    if (content.indexOf("伦理") == -1) {
                        if (title.length > 0) {
                            tmparray.push(obj);
                        }
