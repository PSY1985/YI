   <rule type="101" typeonclickaction="" subtypename="追剧" jsmethod="list();"
     url="http://m.tufutv8.com/Dianshiju/index_%count%_______1.html" currentpagecountholder="%count%" searchkeywordholder=""> 
     <script>
      function list() {
        var root = document.getElementById("vod_list");
        var datalist = root.getElementsByTagName("li");

       if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
