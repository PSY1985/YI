      function list() {
        var datalist = $("ul.myui-vodlist.clearfix").find("li.col-lg-8.col-md-6.col-sm-4.col-xs-3");
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var host = "https://www.mahua110.com";
          var href = $(this).find("a.myui-vodlist__thumb.lazyload").attr("href");
          var title = $(this).find("a.myui-vodlist__thumb.lazyload").attr("title");
          var imgurl = $(this).find("a.myui-vodlist__thumb.lazyload").attr("data-original");
