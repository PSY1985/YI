      function search() {
        var datalist = $("ul.myui-vodlist__media.clearfix").find("li");
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var host = "https://www.mahua110.com";
          var href = $(this).find("a").eq(0).attr("href");
          var title = $(this).find("div.detail").find("h4.title").text();
          var imgurl = $(this).find("a").eq(0).attr("data-original");
          var content = title;
          var obj = {"title": title, "detailUrl": host + href, "posterUrl": imgurl, "content": content};
          if (content.indexOf("伦理") == -1) {
            if (title.length > 0) {
              tmparray.push(obj);
            }
          }
        });
        var response = {"code": "200", "msg": "suc", "content": JSON.stringify(tmparray)};
