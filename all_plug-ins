        var shortText = allText.substring(0, allText.length > 200 ? 200 : allText.length).trim();
        var actor="";
        var allDownList = new Array();
        var allDownTitleTmp = $("div.item.tyui h3.title").toArray();
        var allDownListTmp = $("div.item.tyui ul").toArray();
        for (var i = 0; i < allDownListTmp.length; i++) {
          var titleSort = allDownTitleTmp[i].innerText;
