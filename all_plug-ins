  <rule type="101" typeonclickaction="" subtypename="电影" jsmethod="list();"
    url="http://www.123ku.com/?m=vod-type-id-1-pg-%count%.html" currentpagecountholder="%count%"
    searchkeywordholder="">
    <script>
      function list() {
        var datalist = $("div.xing_vb").find("li");
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var host = "http://www.123ku.com";
          var href = $(this).find("span.xing_vb4").find("a").attr("href");
          var title = $(this).find("span.xing_vb4").find("a").text();
          var imgurl;
          var content = $(this).find("span.xing_vb5").text();
          var obj = {"title": title, "detailUrl": host + href, "posterUrl": imgurl, "content": content, "itemType": 1};
          if (content.indexOf("飛鳥") == -1 && content.indexOf("伦理") == -1) {
            if (title.length > 0) {
              tmparray.push(obj);
            }
          }
        });
        var response = {"code": "200", "msg": "suc", "content": JSON.stringify(tmparray)};
        return response;
      }
    </script>
  </rule>

