                var datalist = document.getElementsByTagName('pre')[0].innerHTML;
                if (!isNaN(datalist)) {
                    var response = {'code': '200', 'msg': 'suc', 'content': ''}
                    return response
                }
                var tmparray = []
                datalist = eval(datalist.substring(8, datalist.length - 1));
                for (var i in datalist) {
                    if (i>9) {
                      break;
                    }
                    var href = ''
                    var title = datalist[i].title.replace(/\s/g, "")
                    var imgurl = datalist[i].cover
                    var content = ''
                    var obj = {'title': title, 'detailUrl': href, 'posterUrl': imgurl, 'content': content, 'itemType': 0, 'clickType': 1}
                    tmparray.push(obj)
                }
                var response = {'code': '200', 'msg': 'suc', 'content': JSON.stringify(tmparray)}
                return response
            }
        </script> 
