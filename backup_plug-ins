    <script>
      function list() {
        var datalist = $("ul#list1.clearfix.tab-pane.fade.in.active").find("li.col-md-2.col-sm-3.col-xs-4");
        if (datalist.length == 0) {
          var response = {"code": "200", "msg": "suc", "content": ""};
          return response;
        }
        var tmparray = new Array();
        $(datalist).each(function () {
          var host = "https://www.nfmovies.com";
          var href = $(this).find("a.videopic.lazy").attr("href");
          var title = $(this).find("a.videopic.lazy").attr("title");
          var imgurl = $(this).find("a.videopic.lazy").attr("data-original");
          var content = title;
          var obj = {"title": title, "detailUrl": host + href, "posterUrl": host + imgurl, "content": content};
          if (content.indexOf("伦理") == -1) {
            if (title.length > 0) {
              tmparray.push(obj);
            }
          }
        });
        var response = {"code": "200", "msg": "suc", "content": JSON.stringify(tmparray)};
        return response;
      }
    </script>
