            var title = downloadTmp[j].getElementsByTagName("a")[0].getAttribute("title");
            var downloadBean = { "name": title, "href": href };
            oneDownList.push(downloadBean);
          }
          var pageDetail = {"title": titleSort, "downlistInfos": oneDownList};
