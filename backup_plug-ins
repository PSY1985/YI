   <rule type="102" jsmethod="detail();"> 
     <script>
      function detail() {
        var host = "https://www.nfmovies.com";
        var posterUrl = "";
        var pageTitle =  $("dd.clearfix").find("h3").text();
        var allText = $("div.plot").text();
        var shortText = allText.substring(0, allText.length > 200 ? 200 : allText.length).trim();
        var actor = $("dd.clearfix").find("ul").find("li").first().text();

        var allDownList = new Array();
        var allDownTitleTmp = $("div#list1.hy-play-list.tab-pane.fade.in.active div.panel.clearfix").toArray();
        var allDownListTmp = $("div#list1.hy-play-list.tab-pane.fade.in.active li").toArray();
        for (var i = 0; i < allDownListTmp.length; i++) {
          var titleSort = allDownTitleTmp[i].getElementsByTagName("a")[0].title;
          var downloadTmp = allDownListTmp[i].getElementsByTagName("a");
          var oneDownList = new Array();
          for (var j = 0; j < downloadTmp.length; j++) {
            var href = downloadTmp[j].getAttribute("href");
            var title = downloadTmp[j].innerHTML;
            var downloadBean = {"name": title, "href": host + href};
            oneDownList.push(downloadBean);
          }
          var pageDetail = {"title": titleSort, "downlistInfos": oneDownList};
          allDownList.push(pageDetail);
        }
        var pageInfo = {"posterUrl": posterUrl, "title": pageTitle, "allText": allText, "shortText": shortText, "summary": actor.replace("undefined", ""), "downlist": allDownList};
        var response = {"code": "200", "msg": "suc", "content": pageInfo};
        return response;
      }
     </script> 
   </rule> 

